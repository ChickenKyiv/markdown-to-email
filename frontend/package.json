  },
  "dependencies": {
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
