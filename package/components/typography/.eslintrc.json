{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "babel-eslint",
    "extends": [
        "plugin:json/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "react/jsx-no-target-blank": 0,
        "comma-dangle": 0,
        "indent": 0,
        "linebreak-style": 0,
        "eol-last": 0
    }
}